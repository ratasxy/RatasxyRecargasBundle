<h1>Pagos</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Monto</th>                <td>{{ entity.monto }}</td>        </tr>
        <tr>
            <th>Tipo</th>                <td>{{ entity.tipo }}</td>        </tr>
        <tr>
            <th>Codigo</th>                <td>{{ entity.codigo }}</td>        </tr>
        <tr>
            <th>Estado</th>                <td>{{ entity.estado }}</td>        </tr>
        <tr>
            <th>Fecha</th>                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('pagos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pagos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('pagos_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
